{
  "version": 2,
  "builds": [
    {
      "src": "netlify/functions/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/netlify/functions/(.*)",
      "dest": "/netlify/functions/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}